<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MyActivity">

    <Button
        android:id="@+id/btn_rok"
        android:layout_width="192dp"
        android:layout_height="43dp"
        android:layout_marginTop="16dp"
        android:text="프로젝트 등록"
        app:layout_constraintEnd_toEndOf="@+id/btn_mypro"
        app:layout_constraintStart_toStartOf="@+id/btn_mypro"
        app:layout_constraintTop_toBottomOf="@+id/btn_mypro" />

    <Button
        android:id="@+id/btn_hyun"
        android:layout_width="192dp"
        android:layout_height="43dp"
        android:layout_marginTop="16dp"
        android:text="후원현황"
        app:layout_constraintEnd_toEndOf="@+id/btn_rok"
        app:layout_constraintStart_toStartOf="@+id/btn_rok"
        app:layout_constraintTop_toBottomOf="@+id/btn_rok" />

    <Button
        android:id="@+id/btn_arlim"
        android:layout_width="192dp"
        android:layout_height="43dp"
        android:layout_marginTop="16dp"
        android:text="알림"
        app:layout_constraintEnd_toEndOf="@+id/btn_hyun"
        app:layout_constraintStart_toStartOf="@+id/btn_hyun"
        app:layout_constraintTop_toBottomOf="@+id/btn_hyun" />

    <Button
        android:id="@+id/btn_seol"
        android:layout_width="192dp"
        android:layout_height="43dp"
        android:layout_marginTop="16dp"
        android:text="설정"
        app:layout_constraintEnd_toEndOf="@+id/btn_arlim"
        app:layout_constraintStart_toStartOf="@+id/btn_arlim"
        app:layout_constraintTop_toBottomOf="@+id/btn_arlim" />

    <Button
        android:id="@+id/btn_back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="16dp"
        android:text="뒤로"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_logout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="32dp"
        android:layout_marginRight="32dp"
        android:text="로그아웃"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_mypro"
        android:layout_width="192dp"
        android:layout_height="43dp"
        android:layout_marginTop="100dp"
        android:text="내가 만든 프로젝트"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>